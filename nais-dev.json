{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "name": "teampeon",
    "namespace": "default",
    "labels": {
      "team": "teampeon"
    }
  },
  "spec": {
    "image": "navikt/tjenestepensjon-simulering:latest",
    "team": "teampeon",
    "port": 8080,
    "ingresses": [
      "https://tjenestepensjon-simulering.nais.preprod.local"
    ],
    "liveness": {
      "path": "isAlive",
      "initialDelay": 10
    },
    "readiness": {
      "path": "isReady",
      "initialDelay": 10
    },
    "vault": {
      "enabled": true
    },
    "replicas": {
      "min": 1,
      "max": 1
    },
    "prometheus": [
      {
        "enabled": true,
        "path": "actuator/prometheus"
      }
    ],
    "resources": {
      "limits": {
        "memory": "512Mi"
      },
      "requests": {
        "memory": "512Mi"
      }
    }
  }
}